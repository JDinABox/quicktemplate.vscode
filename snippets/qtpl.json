{
    "Brackets": {
        "prefix": "{% ",
        "body": "{% $0 %}"
    },
    "Cat": {
        "prefix": "cat",
        "body": "{% cat \"${0:path}\" %}"
    },
    "Code": {
        "prefix": "code",
        "body": ["{% code","$0","%}"]
    },
    "Collapses Space": {
        "prefix": "collapsespace",
        "body": ["{% collapsespace %}", "\t$0", "{% endcollapsespace %}"]
    },
    "Comment": {
        "prefix": "comment",
        "body": ["{% comment %}", "\t$0", "{% endcomment %}"]
    },
    "Else": {
        "prefix": "else",
        "body": "{% else %}$0"
    },
    "ElseIf": {
        "prefix": "elseif",
        "body": "{% elseif $1 %}$0"
    },
    "For": {
        "prefix": "for",
        "body": ["{% for ${1:i} := ${2:0}; $1 < ${3:n}; $1++ %}", "\t$0", "{% endfor %}"]
    },
    "Function": {
        "prefix": "func",
        "body": ["{% func $1($2) %}", "\t$0", "{% endfunc %}"]
    },
    "If": {
        "prefix": "if",
        "body": ["{% if $1 %}", "\t$0", "{% endif %}"]
    },
    "IfElse": {
        "prefix": "ifelse",
        "body": ["{% if $1 %}", "\t$2", "{% else %}", "$0", "{% endif %}"]
    },
    "Import": {
        "prefix": "import",
        "body": "{% import \"$0\" %}"
    },
    "Interface": {
        "prefix": "interface",
        "body": ["{% interface ${1:name} {", "\t$0", "} %}"]
    },
    "Switch": {
        "prefix": "switch",
        "body": ["{% switch $1 %}", "{% case $2 %}","$0","${3:{% default %\\}\n}","{% endswitch %}"]
    },
    "Strip Space": {
        "prefix": "stripspace",
        "body": ["{% stripspace %}", "\t$0", "{% stripspace %}"]
    }
}